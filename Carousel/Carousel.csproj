<Project Sdk="Uno.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net10.0-desktop</TargetFrameworks>

    <OutputType>Exe</OutputType>
    <UnoSingleProject>true</UnoSingleProject>

    <ApplicationTitle>Carousel</ApplicationTitle>
    <ApplicationId>com.companyname.carousel</ApplicationId>
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationPublisher>Carousel</ApplicationPublisher>
    <Description>Carousel control prototype for Uno Platform Toolkit.</Description>

    <UnoFeatures>
      Material;
      Toolkit;
      MVUX;
      SkiaRenderer;
    </UnoFeatures>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Uno.Toolkit.Skia.WinUI" />
  </ItemGroup>
</Project>
